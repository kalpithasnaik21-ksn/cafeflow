{
  "routes": [
    {
      "src": "^/$",
      "dest": "/frontend/index.html"
    },
    {
      "src": "^/customer/(.*)",
      "dest": "/frontend/customer/$1"
    },
    {
      "src": "^/admin/(.*)",
      "dest": "/frontend/admin/$1"
    },
    {
      "src": "^/auth/(.*)",
      "dest": "/frontend/auth/$1"
    },
    {
      "src": "^/css/(.*)",
      "dest": "/frontend/css/$1"
    },
    {
      "src": "^/js/(.*)",
      "dest": "/frontend/js/$1"
    },
    {
      "src": "^/api/(.*)",
      "dest": "/api/index.js"
    }
  ]
}
